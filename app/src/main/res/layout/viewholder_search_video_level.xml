<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <player.wellnesssolutions.com.common.customize_views.MMOptionTextView
        android:id="@+id/viewWrapper"
        android:layout_width="@dimen/vh_search_item_circle_item_size_large"
        android:layout_height="@dimen/vh_search_item_circle_item_size_large"
        android:layout_marginLeft="@dimen/vh_search_item_circle_item_margin_lagre"
        android:layout_marginRight="@dimen/vh_search_item_circle_item_margin_lagre"
        app:layout_constraintBottom_toTopOf="@+id/tvName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="packed"
        app:selectType="selected"
        app:shapeType="circle" />

    <player.wellnesssolutions.com.common.customize_views.MMTextView
        android:id="@+id/tvLevel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/vh_search_item_text_circle_item_text_size"
        app:customFont="@string/font_liberator_heavy"
        app:layout_constraintBottom_toBottomOf="@+id/viewWrapper"
        app:layout_constraintEnd_toEndOf="@+id/viewWrapper"
        app:layout_constraintStart_toStartOf="@+id/viewWrapper"
        app:layout_constraintTop_toTopOf="@+id/viewWrapper"
        android:layout_marginTop="@dimen/margin"
        app:style="btnColor" />

    <player.wellnesssolutions.com.common.customize_views.MMTextView
        android:id="@+id/tvName"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/vh_svb_tv_name_margin_top"
        android:ellipsize="end"
        android:gravity="center"
        android:maxLines="1"
        android:textSize="@dimen/vh_search_item_tv_title_text_size"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@id/viewWrapper"
        app:layout_constraintStart_toStartOf="@id/viewWrapper"
        app:layout_constraintTop_toBottomOf="@+id/viewWrapper"
        app:layout_constraintVertical_chainStyle="packed"
        app:style="btnColor" />

</androidx.constraintlayout.widget.ConstraintLayout>